#include<stdio.h>
#include<conio.h>
void main()
{
    int i,j,k,l;

    for(i=1; i<=10; i++)
    {
        for(j=1; j<=i; j++)
        {
            printf("*");
        }
        for(k=19; k>=(2*i-1); k--)
        {
            printf(" ");
        }
        for(l=1; l<=i; l++)
        {
            printf("*");
        }
        printf("\n");
    }
   // for(i=1; i<=21; i++)
    //{
       // printf("*");
   // }
   // printf("\n");
    for(i=1; i<=10; i++)
    {
        for(j=i; j<=10; j++)
        {
            printf("*");
        }
        for(k=1; k<=(2*i-1); k++)
        {
            printf(" ");
        }
        for(l=10; l>=i; l--)
        {
            printf("*");
        }
        printf("\n");
    }
    getch();
}